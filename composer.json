{
    "name": "schentrup-software/php-api",
    "description": "A basic api framework for PHP. It uses classes for request and responses to make a self documenting API.",
    "keywords": [
        "API",
        "Framewaork",
        "REST"
    ],
    "license": "MIT",
    "version": "1.0.0",
    "authors": [
        {
            "name": "joeySchentrup",
            "email": "joey@schentrupsoftware.com"
        }
    ],
    "type": "library",
    "require": {
        "php": ">=8.1",
        "pmjones/auto-route": "^2.0",
        "sapien/sapien": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "PhpApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpApi\\Tests\\": "tests/"
        }
    }
}