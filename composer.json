{
    "name": "buonzz/composer-library-template",
    "description": "Boilerplate package for creating other packages.",
    "keywords": [
        "template",
        "composer",
        "package"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Darwin Biler",
            "email": "buonzz@gmail.com"
        }
    ],
    "type": "project",
    "require": {
        "php": ">=8.1",
        "pmjones/auto-route": "^2.0",
        "sapien/sapien": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "8.*"
    },
    "autoload": {
        "psr-4": {
            "Buonzz\\Template\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buonzz\\Template\\Tests\\": "tests/"
        }
    }
}
